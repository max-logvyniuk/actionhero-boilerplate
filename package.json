{
  "name": "actionhero_tutorial",
  "description": "actionhero boilerplate",
  "version": "0.1.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@types/node": "^14.14.19",
    "actionhero": "^25.0.2",
    "ah-sequelize-plugin": "^3.0.1",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "ioredis": "^4.19.4",
    "lodash": "^4.17.20",
    "mariadb": "^2.5.2",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.4.0",
    "sequelize-typescript": "^2.0.0",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^26.0.19",
    "axios": "^0.21.1",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1"
  },
  "scripts": {
    "postinstall": "yarn run build",
    "dev": "ts-node-dev --no-deps --transpile-only ./src/server.ts",
    "start": "node ./dist/server.js",
    "actionhero": "actionhero",
    "test": "jest",
    "pretest": "npm run build && npm run lint",
    "build": "tsc --declaration ",
    "lint": "prettier --check src __tests__",
    "pretty": "prettier --write src __tests__",
    "production": "yarn run postinstall && node ./dist/server.js"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
